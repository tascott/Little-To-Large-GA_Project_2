<%= render partial: "layouts/header" %>



  <div class="row">
    <div class="small-12 columns">
      <table>
        <tr>
          <th>Username</th>
          <th>First Name</th>
          <th>Last Name</th>
          <th>ID</th>

        </tr>

        <% @users.each do |user| %>
        <tr class="eventtable">
          <td><%= user.username %></td>
          <td><%= user.first_name %></td>
          <td><%= user.last_name %></td>
          <td><%= user.id %></td>
        </tr>

        <% end %>

      </table>
    </div>
  </div>

</div>
